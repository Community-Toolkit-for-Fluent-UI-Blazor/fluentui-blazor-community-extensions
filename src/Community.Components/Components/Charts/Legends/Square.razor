@namespace FluentUI.Blazor.Community.Components
@inherits FluentComponentBase


<svg id="@Id"
	 @ref="Element"
	 @key="@Id"
	 class="@Class"
	 style="@Style"
	 xmlns="http://www.w3.org/2000/svg"
	 width="@Size"
	 height="@Size"
	 viewBox="0 0 @Size @Size">
	<rect x="@StrokeWidth"
		  y="@StrokeWidth"
		  width="@Size - 2*@StrokeWidth"
		  height="@Size - 2*@StrokeWidth"
		  fill="@Fill"
		  stroke="@Stroke"
		  stroke-width="@StrokeWidth"
		  opacity="@Opacity"/>
</svg>
