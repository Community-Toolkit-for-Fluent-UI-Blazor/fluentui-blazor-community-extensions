@namespace FluentUI.Blazor.Community.Components
@inherits FluentComponentBase
@typeparam T where T : ITrace

@code {
    public ChartBase()
    {
        _renderChart = __builder =>
        {
            <PlotlyChart style="@InternalStyle"
                         @bind-Config=Config
                         @bind-Layout=Layout
                         Data=@GetTraces()
                         DataChanged="@OnDataChangedAsync"/>
        };
    }
}
